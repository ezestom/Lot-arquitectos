{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"baseUrl": ".",
		"paths": {
			"@components/*": ["src/components/*"],
			"@ui/*": ["src/components/ui/*"],
			"@layouts/*": ["src/layouts/*"],
			"@icons/*": ["src/icons/*"],
			"@lib/*": ["src/lib/*"],
			"@types": ["src/types.ts"]
		}
	}
}
